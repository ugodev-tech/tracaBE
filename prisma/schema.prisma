model User {
  id          Int      @id @default(autoincrement())
  email       String   @unique
  name        String
  phone       String
  password    String
  is_verified Boolean  @default(false)
  role        Role     @default(USER)
  created_at  DateTime @default(now())
  updated_at  DateTime @updatedAt
}

model Token {
  id         Int      @id @default(autoincrement())
  email      String
  token      String?
  created_at DateTime @default(now())
  expires_at DateTime
}

enum Role {
  USER
  ADMIN
}
datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
  }
  
generator client {
  provider = "prisma-client-js"
}
